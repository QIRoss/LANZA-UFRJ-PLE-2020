LIBARQUIVOSOBJS = aula0801.o
LIBARQUIVOS = libarquivos.a

EXECS = $(AULA01)\
		$(AULA02)\
		$(AULA03)\
		$(AULA04)\
		$(AULA05)\
		$(AULA06)\
		$(AULA07)\
		$(AULA08)

LIBS = $(LIBMATEMATICA)

ALL = $(EXECS) $(LIBS)

#Regra implicita
.c.o:
	$(CC) $(CFLAGS) -c $<

all: $(ALL)

aula01: $(AULA01)

