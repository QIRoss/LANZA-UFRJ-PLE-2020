OS = `uname -s`

CC = gcc

.ifdef CLANG
CC = clang
.endif

LD = $(CC)

CFLAGS = -Wall -std=c11 -D_MY_DEBUG_

.ifndef _MY_DEBUG_
CFLAGS = -Wall -std=c11
.endif

AR = ar

LFLAGS = -Wall -std=c11
AFLAGS = -r

